@model Cultivo.Web.Models.LoginViewModel

@{
    ViewData["Title"] = "Logout";
}

<h1>Logout</h1>

<h4>LoginViewModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Logout">
